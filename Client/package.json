    "material-icons": "^1.13.12",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.0.1",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "require": "^2.4.20",
